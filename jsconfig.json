{
  "module": "es6",
  "moduleResolution": "node",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
    }
  },
}